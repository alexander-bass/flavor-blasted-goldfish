<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fbg.fittrack.NewExerciseController">
   <children>
      <TextField fx:id="nameTextField" layoutX="219.0" layoutY="90.0" promptText="name" />
      <TextField fx:id="setsTextField" layoutX="219.0" layoutY="137.0" promptText="sets" />
      <TextField fx:id="repsTextField" layoutX="219.0" layoutY="193.0" promptText="reps" />
      <TextField fx:id="weightTextField" layoutX="213.0" layoutY="252.0" promptText="weight" />
      <Button fx:id="okButton" layoutX="381.0" layoutY="340.0" mnemonicParsing="false" onAction="#onOkButtonClick" text="Ok" />
      <Button fx:id="cancelButton" layoutX="475.0" layoutY="340.0" mnemonicParsing="false" onAction="#onCancelButtonClick" text="Cancel" />
   </children>
</AnchorPane>
